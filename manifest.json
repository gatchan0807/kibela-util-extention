{
  "manifest_version": 2,
  "name": "name",
  "version": "1.0",
  "description": "description",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "default_title",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.kibe.la/*"
      ],
      "js": [
        "contentScripts/index.js"
      ]
    }
  ]
}